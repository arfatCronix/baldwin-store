{
  "components/common/social-links": "{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            {{#or (if display_name '===' 'twitter') (if display_name '===' 'stumbleupon')}}\r\n                {{#if display_name '===' 'twitter'}}\r\n                    <li class=\"socialLinks-item\">\r\n                        <a class=\"icon icon--x\"\r\n                        href=\"{{url}}\"\r\n                        target=\"_blank\"\r\n                        rel=\"noopener\"\r\n                        title=\"X\"\r\n                        >\r\n                            <span class=\"aria-description--hidden\">X</span>\r\n                            <svg>\r\n                                <use href=\"#icon-x\"/>\r\n                            </svg>\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                {{#if display_name '===' 'stumbleupon'}}\r\n                    <li class=\"socialLinks-item\">\r\n                        <a class=\"icon icon--houzz\"\r\n                        href=\"{{url}}\"\r\n                        target=\"_blank\"\r\n                        rel=\"noopener\"\r\n                        title=\"Houzz\"\r\n                        >\r\n                            <span class=\"aria-description--hidden\">Houzz</span>\r\n                            <svg>\r\n                                <use href=\"#icon-houzz\"/>\r\n                            </svg>\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n            {{else}}\r\n                <li class=\"socialLinks-item\">\r\n                    <a class=\"icon icon--{{display_name}}\"\r\n                    href=\"{{url}}\"\r\n                    target=\"_blank\"\r\n                    rel=\"noopener\"\r\n                    title=\"{{{capitalize display_name}}}\"\r\n                    >\r\n                        <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\r\n                        <svg>\r\n                            <use href=\"#icon-{{display_name}}\"/>\r\n                        </svg>\r\n                    </a>\r\n                </li>\r\n            {{/or}}\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n"
}